<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prb.fitnessproject"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/singleicon"
        android:theme="@android:style/Theme.NoTitleBar" android:largeHeap="true" android:label="@string/app_name">
         <activity 
            android:name="com.prb.fitnessproject.Intro"
            android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.prb.fitnessproject.MainActivity" android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation|screenSize">
        </activity>
        <activity android:name="Start" android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <activity android:name="Developer" android:screenOrientation="portrait" android:configChanges="keyboardHidden|orientation|screenSize"></activity>       
    </application>

</manifest>
